{
    "name": "delt4nin3/laravel-database-trigger",
    "description": "A package to add database trigger to laravel migrations",
    "keywords": [
        "NtimYeboah",
        "trigger",
        "database",
        "laravel-database-trigger",
        "DariusIII",
        "Delt4Nin3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ntim Yeboah",
            "email": "ntimobedyeboah@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Alexander Anding",
            "email": "alexander.anding@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^5.8|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5 || ^7.0 || ^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Delt4Nin3\\LaravelDatabaseTrigger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Delt4Nin3\\LaravelDatabaseTrigger\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Delt4Nin3\\LaravelDatabaseTrigger\\TriggerServiceProvider"
            ]
        }
    }
}
